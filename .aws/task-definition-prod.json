{
  "family": "zigg_spring_prod_td",
  "containerDefinitions": [
    {
      "name": "zigg-service-prod",
      "image": "975050264794.dkr.ecr.ap-northeast-2.amazonaws.com/zigg-service-prod:latest",
      "cpu": 2048,
      "memory": 6144,
      "memoryReservation": 3072,
      "portMappings": [
        {
          "name": "zigg-service-prod-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "zigg-database-port",
          "containerPort": 3306,
          "hostPort": 3306,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "zigg-server-port",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/zigg_spring_prod_td",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    },
    {
      "name": "aws-otel-collector",
      "image": "public.ecr.aws/aws-observability/aws-otel-collector:v0.40.1",
      "cpu": 0,
      "portMappings": [],
      "essential": true,
      "command": [
        "--config=/etc/ecs/ecs-cloudwatch-xray.yaml"
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-aws-otel-sidecar-collector",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::975050264794:role/ecsTaskExcutionRole",
  "executionRoleArn": "arn:aws:iam::975050264794:role/ecsTaskExcutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "6144",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}